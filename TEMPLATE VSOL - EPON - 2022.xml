                <item>
                    <name>ONUs OFFLINE</name>
                    <type>DEPENDENT</type>
                    <key>onu.offline</key>
                    <delay>0</delay>
                    <history>30d</history>
                    <trends>90d</trends>
                    <applications>
                        <application>
                            <name>TOTAIS ONUs</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>function qtd_clientes(json,status){
     var count=0;
     for (var index in json){
       var statusONU = json[index][&quot;{#STATUS}&quot;];
       
       if (statusONU == status){
         count+=1;
       }
     }
     return count;
}

var file = JSON.parse(value);
/* {#STATUS} 1 - UP | 0 - DOWN | -1 - INVALID */
var json = qtd_clientes(file,0);
return JSON.stringify(json);</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>qtd.onu</key>
                    </master_item>
                </item>
