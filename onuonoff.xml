    <date>2021-09-23T17:10:13Z</date>
    <groups>
        <group>
            <name>GPON</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Discovery Huawei Pons</template>
            <name>Discovery Huawei Pons</name>
            <templates>
                <template>
                    <name>ICMP Ping</name>
                </template>
            </templates>
            <groups>
                <group>
                    <name>GPON</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>★Quantidade ONTs/PON★</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>Quantidade ONTs por PON</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#OLTPORT},IF-MIB::ifName]</snmp_oid>
                    <key>olt_interfaces</key>
                    <delay>60m</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#OLTPORT}</macro>
                                <value>^GPON</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>{#OLTPORT}  - Offline</name>
                            <key>qt_off[{$SNMP_COMMUNITY},{$IPOLT},{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <history>5d</history>
                            <trends>7d</trends>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>★Quantidade ONTs/PON★</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>{#OLTPORT}  - Online</name>
                            <key>qt_on[{$SNMP_COMMUNITY},{$IPOLT},{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <history>5d</history>
                            <trends>7d</trends>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>★Quantidade ONTs/PON★</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>{#OLTPORT} - Total</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.2011.6.128.1.1.2.21.1.16.{#SNMPINDEX}</snmp_oid>
                            <key>qt_total[{#OLTPORT}]</key>
                            <delay>5m</delay>
                            <history>5d</history>
                            <trends>7d</trends>
                            <applications>
                                <application>
                                    <name>★Quantidade ONTs/PON★</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
